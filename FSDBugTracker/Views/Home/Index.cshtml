@using FSDBugTracker.Helpers;

        <div class="page-content">
            <!-- Page Breadcrumb -->
            <div class="page-breadcrumbs">
                <ul class="breadcrumb">
                   @if (User.IsInRole("Admin") || (User.IsInRole("SuperUser")))
                   {
                    <li style="color:black;"><a href="@Url.Action("SeedDataAsync")">Seed Database</a></li>
                   }
                    <li>
                        <i class="fa fa-home"></i>
                        <a href="@Url.Action("Index","Home")">Home</a>
                    </li>
                    <li class="active">Dashboard</li>
                </ul>
            </div>            
            <!-- /Page Breadcrumb -->
            <!-- Page Header -->
            <div class="page-header position-relative">
                <div class="header-title">
                    <h1>
                        Dashboard
                    </h1>
                </div>
                <!--Header Buttons-->
                <div class="header-buttons">
                    <a class="sidebar-toggler" href="#">
                        <i class="fa fa-arrows-h"></i>
                    </a>
                    <a class="refresh" id="refresh-toggler" href="">
                        <i class="glyphicon glyphicon-refresh"></i>
                    </a>
                    <a class="fullscreen" id="fullscreen-toggler" href="#">
                        <i class="glyphicon glyphicon-fullscreen"></i>
                    </a>
                </div>
                <!--Header Buttons End-->
            </div>
            <!-- /Page Header -->
            <!-- Page Body -->
            <div class="page-body">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <div class="databox bg-white radius-bordered">
                                    <div class="databox-left bg-themesecondary">
                                        <div class="databox-piechart">
                                            <div data-toggle="easypiechart" class="easyPieChart" data-barcolor="#fff" data-linecap="butt" data-percent="50" data-animate="500" data-linewidth="3" data-size="47" data-trackcolor="rgba(255,255,255,0.1)"><span class="white font-90"></span></div>
                                        </div>
                                    </div>
                                    <div class="databox-right">
                                        <span class="databox-number themesecondary">@ViewBag.AllUnarchived</span>
                                        <div class="databox-text darkgray">ALL TICKETS</div>
                                        <div class="databox-stat themesecondary radius-bordered">
                                            <i class="stat-icon icon-lg fa fa-tasks"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <div class="databox bg-white radius-bordered">
                                    <div class="databox-left bg-themethirdcolor">
                                        <div class="databox-piechart">
                                            <div data-toggle="easypiechart" class="easyPieChart" data-barcolor="#fff" data-linecap="butt" data-percent="15" data-animate="500" data-linewidth="3" data-size="47" data-trackcolor="rgba(255,255,255,0.2)"><span class="white font-90"></span></div>
                                        </div>
                                    </div>
                                    <div class="databox-right">
                                        <span class="databox-number themethirdcolor">@ViewBag.AllOpen</span>
                                        <div class="databox-text darkgray">UNASSIGNED TICKETS</div>
                                        <div class="databox-stat themethirdcolor radius-bordered">
                                            <i class="stat-icon  icon-lg fa fa-envelope-o"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <div class="databox bg-white radius-bordered">
                                    <div class="databox-left bg-themeprimary">
                                        <div class="databox-piechart">
                                            <div id="users-pie" data-toggle="easypiechart" class="easyPieChart" data-barcolor="#fff" data-linecap="butt" data-percent="76" data-animate="500" data-linewidth="3" data-size="47" data-trackcolor="rgba(255,255,255,0.1)"><span class="white font-90"></span></div>
                                        </div>
                                    </div>
                                    <div class="databox-right">
                                        <span class="databox-number themeprimary">@ViewBag.AllArchived</span>
                                        <div class="databox-text darkgray">ASSIGNED TICKETS</div>
                                        <div class="databox-state bg-themeprimary">
                                            <i class="fa fa-tags"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <div class="databox bg-white radius-bordered">
                                    <div class="databox-left bg-themefifthcolor">
                                        <div class="databox-piechart">
                                            <div id="users-pie" data-toggle="easypiechart" class="easyPieChart" data-barcolor="#fff" data-linecap="butt" data-percent="76" data-animate="500" data-linewidth="3" data-size="47" data-trackcolor="rgba(255,255,255,0.1)"><span class="white font-90"></span></div>
                                        </div>
                                    </div>
                                    <div class="databox-right">
                                        <span class="databox-number themeprimary">@ViewBag.AllClosed</span>
                                        <div class="databox-text darkgray">RESOLVED TICKETS</div>
                                        <div class="databox-state bg-themeprimary">
                                            <i class="fa fa-check"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>   
                
                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <div class="widget">
                        <div class="widget-header bg-orange">
                            <span class="widget-caption">Precentage of Total Tickets that are currently "Open"</span>
                            <div class="widget-buttons">
                                <div class="progress progress-bordered progress-striped active" style="width:150px;">
                                    <div class="progress-bar progress-bar-darkorange" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: @ViewBag.AA%">
                                        <span class="sr-only">
                                            20% Complete (success)
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="widget-body">

                            <p>
                                This is to advise the Admin and Project Managers the the STATUS of the ticket queues.
                            </p>

                        </div>
                    </div>
                </div>

                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <div class="widget">
                        <div class="widget-header bg-yellow">
                            <span class="widget-caption">Precentage of all Tickets that are currently "Resolved"</span>
                            <div class="widget-buttons">
                                <div class="progress progress-bordered progress-striped active" style="width:150px;">
                                    <div class="progress-bar progress-bar-darkorange" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: @ViewBag.AC%" >
                                        <span class="sr-only">
                                            20% Complete (success)
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="widget-body">

                            <p>
                                This is to advise the Admin and Project Managers the PACE by which tickets in the queue get Resolved.
                            </p>

                        </div>
                    </div>
                </div>

                <div class="col-lg-6 col-sm-6 col-xs-12">
                    <div class="widget">
                        <div class="widget-header bg-blue">
                            <span class="widget-caption">Precentage of all "Open" Tickets that are currently "Unassigned"</span>
                            <div class="widget-buttons">
                                <div class="progress progress-bordered progress-striped active" style="width:150px;">
                                    <div class="progress-bar progress-bar-darkorange" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: @ViewBag.AO%">
                                        <span class="sr-only">
                                            20% Complete (success)
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="widget-body">

                            <p>
                                This is to advise the Admin and Project Managers how many tickets have yet to be ADDRESSED by the Developers.
                            </p>

                        </div>
                    </div>
                </div>

                @*<div class="col-lg-6 col-sm-6 col-xs-12">
                    <div class="widget">
                        <div class="widget-header bg-magenta">
                            <span class="widget-caption">Colored Widget with Bordered ProgressBar</span>
                            <div class="widget-buttons">
                                <div class="progress progress-bordered progress-striped active" style="width:150px;">
                                    <div class="progress-bar progress-bar-darkorange" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: @ViewBag.AC%">
                                        <span class="sr-only">
                                            20% Complete (success)
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="widget-body">

                            <p>
                                Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.
                            </p>

                        </div>
                    </div>
                </div>*@
            </div>
            <!-- /Page Body -->
        </div>
        <!-- /Page Content -->




